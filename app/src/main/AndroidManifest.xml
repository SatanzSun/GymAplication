<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.mysqlproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="MySGL"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MySQLproject">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterActivity">
            <intent-filter>
                <action android:name="package.RegisterActivity" />

                <category android:name="android.intent.category.DEFAULT"></category>
            </intent-filter>
        </activity>
        <activity android:name=".HomeScreene"></activity>
        <activity
            android:name=".HelpScreen"
            android:parentActivityName=".HomeScreene"></activity>
        <activity android:name=".History"></activity>
        <activity android:name=".RunningExcercise">

        </activity>
        <activity android:name=".AddExerciseOrEndSession"
            android:parentActivityName=".HomeScreene">

        </activity>
        <activity android:name=".ExerciseList">

        </activity>
        <activity android:name=".SingleSessionLog">
            
        </activity>
        <activity android:name=".LoginActivity">

        </activity>




        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>